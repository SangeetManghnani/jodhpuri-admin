{
    "userId": {
        "type": "string",
        "minLen": 1
    },
    "orderDate": {
        "type": "string",
        "minLen": 1
    },
    "orderState": {
        "type": "string",
        "minLen": 1
    },
    "paymentMode": {
        "type": "string",
        "minLen": 1
    },
    "paymentStatus": {
        "type": "string",
        "minLen": 1
    },
    "products": {
        "type": "array",
