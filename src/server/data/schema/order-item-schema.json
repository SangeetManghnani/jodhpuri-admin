                "count": {
                    "type": "number"
                },
                "total": {
                    "type": "number"
                },
                "required": ["id", "count", "total"]
            }
        }
    },
    "totalValue": {
        "type": "number"
    },
    "taxApplicable": {
        "type": "number"
    },
    "finalPayable": {
        "type": "number"
    },
