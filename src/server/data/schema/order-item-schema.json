                        "type": "object",
                        "properties": {
                            "houseNo": {
                                "type": "string",
                                "minLen": 1
                            },
                            "addressLine1": {
                                "type": "string",
                                "minLen": 1
                            },
                            "addressLine2": {
                                "type": "string"
                            },
                            "landmark": {
                                "type": "string"
                            },
                            "city": {
                                "type": "string",
                                "minLen": 1
                            },
                            "state": {
                                "type": "string",
                                "minLen": 1
                            },
                            "pin": {
                                "type": "string",
                                "minLen": 1
                            },
                            "required": ["houseNo", "addressLine1", "city", "state", "pin"]
                        }
                    }
                },
                "required": ["contactName", "contactNumber", "address"]
